{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "sourceMap": true
  },
  "extends": "@tsconfig/node14/tsconfig.json",
  "baseUrl": ".",
  "paths": {
    "@etimo-achievements/types/*": [
      "types/src/*"
    ],
    "@etimo-achievements/security/*": [
      "security/src/*"
    ],
    "@etimo-achievements/common/*": [
      "common/src/*"
    ],
    "@etimo-achievements/api/*": [
      "api/src/*"
    ],
    "@etimo-achievements/service/*": [
      "service/src/*"
    ],
    "@etimo-achievements/data/*": [
      "data/src/*"
    ]
  }
}
