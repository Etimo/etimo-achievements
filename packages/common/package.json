{
  "name": "@etimo-achievements/common",
  "version": "1.0.0",
  "description": "Common useful functions",
  "author": "Niclas Lindstedt <niclas.lindstedt@etimo.se>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "clear": "npm run clean && rimraf node_modules",
    "compile": "tsc --build",
    "start": "ts-node src/index.ts",
    "test-integration": "exit 0; jest --preset ts-jest --env node --testMatch \"**/src/**/*.spec.ts\" --modulePathIgnorePatterns dist",
    "test": "exit 0; jest --preset ts-jest --env node --testMatch \"**/src/**/*.test.ts\" --modulePathIgnorePatterns dist"
  },
  "dependencies": {
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/uuid": "^8.3.1",
    "jest": "^27.2.5",
    "ts-jest": "^27.0.6",
    "typescript": "^4.4.3"
  }
}
