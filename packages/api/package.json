{
  "name": "@etimo-achievements/api",
  "version": "1.0.0",
  "description": "Authenticates a user with OAuth2",
  "author": "Niclas Lindstedt <niclas.lindstedt@etimo.se>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "clear": "npm run clean && rimraf node_modules",
    "compile": "tsc --build; cp -f package.json dist/",
    "dev": "nodemon",
    "start": "ts-node src/index.ts",
    "test-integration": "exit 0; jest --preset ts-jest --env node --testMatch \"**/src/**/*.spec.ts\" --modulePathIgnorePatterns dist",
    "test": "jest --preset ts-jest --env node --testMatch \"**/src/**/*.test.ts\" --modulePathIgnorePatterns dist"
  },
  "dependencies": {
    "@etimo-achievements/common": "^1.0.0",
    "@etimo-achievements/data": "^1.0.0",
    "@etimo-achievements/security": "^1.0.0",
    "@etimo-achievements/service": "^1.0.0",
    "express": "^4.17.1",
    "express-winston": "^4.2.0",
    "joi": "^17.4.2",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "jest": "^27.2.5",
    "nodemon": "^2.0.13",
    "ts-jest": "^27.0.6",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
